{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": "> 0.25%, not dead"
    }],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/proposal-numeric-separator",
    "@babel/transform-runtime",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}